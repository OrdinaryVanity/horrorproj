// checks for collision against ground and other solid actors
Class Nash_FloorCheck : Actor
{
	Default
	{
		Radius 16;
		Height 4;
		+SOLID;
	}
	
	States
	{
		Spawn:
			TNT1 A 1;
			Stop;
	}
}

